home/${encodeURIComponent(this.selectedCategory)}`);\n        const result = res.data;\n        if (result.status === 200 && !result.errorCode) {\n          this.homeData = result.data;\n        } else {\n          console.error(\"카테고리별 홈 데이터 조회 실패:\", result);\n        }\n      } catch (err) {\n        console.error(\"카테고리별 홈 데이터 조회 오류:\", err);\n      }\n    },\n    goToPlaces(accommodationId) {\n      console.log(\"Navigating to:\", `/places/${accommodationId}/${this.checkInDate}/${this.checkOutDate}`);\n      this.$router.push(`/places/${accommodationId}/${this.checkInDate}/${this.checkOutDate}`);\n    }\n  }\n};","map":{"version":3,"names":["api","name","data","homeData","selectedCategory","selectedLocation","checkInDate","Date","toISOString","split","checkOutDate","now","computed","isLoggedIn","localStorage","getItem","created","fetchHomeData","methods","response","get","result","status","errorCode","console","log","error","message","goToLocationHome","res","encodeURIComponent","err","logout","removeItem","$router","push","goToSubHome","goToPlaces","accommodationId"],"sources":["C:\\Users\\mycoms\\Desktop\\study\\Ducktel\\duckfront\\src\\components\\HomePage.vue"],"sourcesContent":["<template>\r\n  <div class=\"home-container\">\r\n    <header>\r\n      <h2>홈페이지</h2>\r\n    </header>\r\n    <div class=\"filter-row\">\r\n      <div class=\"filter-item\">\r\n      <label for=\"category\">카테고리 선택:</label>\r\n      <select id=\"category\" v-model=\"selectedCategory\" @change=\"goToSubHome\">\r\n        <option value=\"\" disabled>카테고리를 선택하세요</option>\r\n        <option value=\"호텔\">호텔</option>\r\n        <option value=\"리조트\">리조트</option>\r\n        <option value=\"펜션\">펜션</option>\r\n        <option value=\"게스트하우스\">게스트하우스</option>\r\n        <option value=\"모텔\">모텔</option>\r\n      </select>\r\n    </div>\r\n      <div class=\"filter-item\" v-if=\"selectedCategory\">\r\n      <label for=\"location\">위치 선택:</label>\r\n      <select id=\"location\"\r\n              v-model=\"selectedLocation\"\r\n              @change=\"goToLocationHome\">\r\n        <option value=\"\" disabled>위치를 선택하세요</option>\r\n        <option value=\"서울\">서울</option>\r\n        <option value=\"부산\">부산</option>\r\n        <option value=\"제주\">제주</option>\r\n      </select>\r\n    </div>\r\n    </div>\r\n\r\n    <!-- 할인 숙소 -->\r\n    <div v-if=\"homeData && homeData.discountAccommodations\">\r\n      <h3>할인 숙소</h3>\r\n      <div class=\"accommodations-container\">\r\n        <div\r\n          v-for=\"item in homeData.discountAccommodations\"\r\n          :key=\"item.accommodationId\"\r\n          class=\"accommodation-card\"\r\n          @click=\"goToPlaces(item.accommodationId)\"\r\n        >\r\n          <img\r\n            v-if=\"item.image && item.image.length\"\r\n            :src=\"item.image[0]\"\r\n            alt=\"숙소 이미지\"\r\n            class=\"accommodation-image\"\r\n          />\r\n          <div class=\"accommodation-details\">\r\n            <strong>{{ item.name }}</strong>\r\n            <p>{{ item.location }}</p>\r\n            <p v-if=\"item.discount\">할인: {{ item.discount }}%</p>\r\n            <p>좋아요: {{ item.likeCount }}</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 인기 숙소 -->\r\n    <div v-if=\"homeData && homeData.favoriteAccommodations\">\r\n      <h3>인기 숙소</h3>\r\n      <div class=\"accommodations-container\">\r\n        <div\r\n          v-for=\"item in homeData.favoriteAccommodations\"\r\n          :key=\"item.accommodationId\"\r\n          class=\"accommodation-card\"\r\n          @click=\"goToPlaces(item.accommodationId)\"\r\n        >\r\n          <img\r\n            v-if=\"item.image && item.image.length\"\r\n            :src=\"item.image[0]\"\r\n            alt=\"숙소 이미지\"\r\n            class=\"accommodation-image\"\r\n          />\r\n          <div class=\"accommodation-details\">\r\n            <strong>{{ item.name }}</strong>\r\n            <p>{{ item.location }}</p>\r\n            <p v-if=\"item.discount\">할인: {{ item.discount }}%</p>\r\n            <p>좋아요: {{ item.likeCount }}</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 계절 추천 숙소 -->\r\n    <div v-if=\"homeData && homeData.seasonalAccommodations\">\r\n      <h3>계절 추천 숙소</h3>\r\n      <div class=\"accommodations-container\">\r\n        <div\r\n          v-for=\"item in homeData.seasonalAccommodations\"\r\n          :key=\"item.accommodationId\"\r\n          class=\"accommodation-card\"\r\n          @click=\"goToPlaces(item.accommodationId)\"\r\n        >\r\n          <img\r\n            v-if=\"item.image && item.image.length\"\r\n            :src=\"item.image[0]\"\r\n            alt=\"숙소 이미지\"\r\n            class=\"accommodation-image\"\r\n          />\r\n          <div class=\"accommodation-details\">\r\n            <strong>{{ item.name }}</strong>\r\n            <p>{{ item.location }}</p>\r\n            <p v-if=\"item.discount\">할인: {{ item.discount }}%</p>\r\n            <p>좋아요: {{ item.likeCount }}</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <p v-if=\"!homeData\">데이터를 불러오는 중...</p>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport api from \"../api\";\r\n\r\nexport default {\r\n  name: \"HomePage\",\r\n  data() {\r\n    return {\r\n      homeData: null,\r\n      selectedCategory: \"\",\r\n      selectedLocation: \"\",\r\n      checkInDate: new Date().toISOString().split(\"T\")[0],\r\n      checkOutDate: new Date(Date.now() + 86400000).toISOString().split(\"T\")[0],\r\n    };\r\n  },\r\n  computed: {\r\n    isLoggedIn() {\r\n      return !!localStorage.getItem(\"accessToken\");\r\n    },\r\n  },\r\n  created() {\r\n    this.fetchHomeData();\r\n  },\r\n  methods: {\r\n    async fetchHomeData() {\r\n      try {\r\n        const response = await api.get(\"/api/home\");\r\n        const result = response.data;\r\n        if (result.status === 200 && !result.errorCode) {\r\n          this.homeData = result.data;\r\n          console.log(\"홈 데이터 조회 성공:\", this.homeData);\r\n        } else {\r\n          console.error(\"홈 데이터 조회 실패:\", result);\r\n        }\r\n      } catch (error) {\r\n        console.error(\"홈 데이터 조회 오류:\", error.response?.data || error.message);\r\n      }\r\n    },\r\n    async goToLocationHome() {\r\n      if (!this.selectedCategory || !this.selectedLocation) return;\r\n\r\n      try {\r\n        const res = await api.get(\r\n            `/api/home/${encodeURIComponent(this.selectedCategory)}/${encodeURIComponent(this.selectedLocation)}`\r\n        );\r\n        const result = res.data;\r\n        if (result.status === 200 && !result.errorCode) {\r\n          this.homeData = result.data;\r\n          console.log(\"위치별 홈 데이터 조회 성공:\", this.homeData);\r\n        } else {\r\n          console.error(\"위치별 홈 데이터 조회 실패:\", result);\r\n        }\r\n      } catch (err) {\r\n        console.error(\"위치별 홈 데이터 조회 오류:\", err.response?.data || err.message);\r\n      }\r\n    },\r\n    logout() {\r\n      localStorage.removeItem(\"accessToken\");\r\n      localStorage.removeItem(\"refreshToken\");\r\n      this.$router.push(\"/\");\r\n    },\r\n    async goToSubHome() {\r\n      if (!this.selectedCategory) return;\r\n\r\n      try {\r\n        const res = await api.get(`/api/home/${encodeURIComponent(this.selectedCategory)}`);\r\n        const result = res.data;\r\n        if (result.status === 200 && !result.errorCode) {\r\n          this.homeData = result.data;\r\n        } else {\r\n          console.error(\"카테고리별 홈 데이터 조회 실패:\", result);\r\n        }\r\n      } catch (err) {\r\n        console.error(\"카테고리별 홈 데이터 조회 오류:\", err);\r\n      }\r\n    },\r\n    goToPlaces(accommodationId) {\r\n      console.log(\"Navigating to:\", `/places/${accommodationId}/${this.checkInDate}/${this.checkOutDate}`);\r\n      this.$router.push(`/places/${accommodationId}/${this.checkInDate}/${this.checkOutDate}`);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.