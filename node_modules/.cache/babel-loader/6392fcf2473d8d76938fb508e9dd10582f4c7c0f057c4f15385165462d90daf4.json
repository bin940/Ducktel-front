{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport api from \"../api\";\nexport default {\n  name: \"HomePage\",\n  data() {\n    return {\n      homeData: null\n    };\n  },\n  computed: {\n    isLoggedIn() {\n      const token = localStorage.getItem(\"accessToken\");\n      console.log(\"홈페이지 - 로그인 상태 확인, accessToken:\", token);\n    }\n  },\n  created() {\n    this.fetchHomeData();\n  },\n  methods: {\n    async fetchHomeData() {\n      try {\n        const response = await api.get(\"/api/home\");\n        const result = response.data;\n        if (result.status === 200 && !result.errorCode) {\n          this.homeData = result.data;\n          console.log(\"홈 데이터 조회 성공:\", this.homeData);\n        } else {\n          console.error(\"홈 데이터 조회 실패:\", result);\n        }\n      } catch (error) {\n        console.error(\"홈 데이터 조회 오류:\", error.response?.data || error.message);\n      }\n    },\n    logout() {\n      localStorage.removeItem(\"accessToken\");\n      localStorage.removeItem(\"refreshToken\");\n      this.$router.push(\"/\");\n      window.location.reload();\n    }\n  }\n};","map":{"version":3,"names":["api","name","data","homeData","computed","isLoggedIn","token","localStorage","getItem","console","log","created","fetchHomeData","methods","response","get","result","status","errorCode","error","message","logout","removeItem","$router","push","window","location","reload"],"sources":["C:\\Users\\mycoms\\Desktop\\study\\duckfront\\src\\components\\HomePage.vue"],"sourcesContent":["<template>\r\n  <div class=\"home-container\">\r\n    <header>\r\n      <nav>\r\n        <router-link v-if=\"!isLoggedIn\" to=\"/login\">로그인</router-link>\r\n        <router-link v-if=\"!isLoggedIn\" to=\"/register\">회원가입</router-link>\r\n        <template v-if=\"isLoggedIn\">\r\n          <router-link to=\"/mypage\">마이페이지</router-link> |\r\n          <a href=\"#\" @click.prevent=\"logout\">로그아웃</a>\r\n        </template>\r\n      </nav>\r\n    </header>\r\n    <h2>홈 페이지</h2>\r\n    <div v-if=\"homeData\">\r\n      <h3 v-if=\"homeData.discountAccommodations?.length\">할인 숙소</h3>\r\n      <ul v-if=\"homeData.discountAccommodations?.length\">\r\n        <li v-for=\"item in homeData.discountAccommodations\" :key=\"item.accommodationId\">\r\n          {{ item.name }} - {{ item.location }} (할인: {{ item.discount }}%)\r\n        </li>\r\n      </ul>\r\n      <h3 v-if=\"homeData.favoriteAccommodations?.length\">인기 숙소</h3>\r\n      <ul v-if=\"homeData.favoriteAccommodations?.length\">\r\n        <li v-for=\"item in homeData.favoriteAccommodations\" :key=\"item.accommodationId\">\r\n          {{ item.name }} - {{ item.location }} (좋아요: {{ item.likeCount }})\r\n        </li>\r\n      </ul>\r\n      <h3 v-if=\"homeData.seasonalAccommodations?.length\">계절별 숙소</h3>\r\n      <ul v-if=\"homeData.seasonalAccommodations?.length\">\r\n        <li v-for=\"item in homeData.seasonalAccommodations\" :key=\"item.accommodationId\">\r\n          {{ item.name }} - {{ item.season }}\r\n        </li>\r\n      </ul>\r\n    </div>\r\n    <p v-else>데이터를 불러오는 중...</p>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport api from \"../api\";\r\n\r\nexport default {\r\n  name: \"HomePage\",\r\n  data() {\r\n    return {\r\n      homeData: null,\r\n    };\r\n  },\r\n  computed: {\r\n    isLoggedIn() {\r\n      const token = localStorage.getItem(\"accessToken\");\r\n      console.log(\"홈페이지 - 로그인 상태 확인, accessToken:\", token); \r\n    },\r\n  },\r\n  created() {\r\n    this.fetchHomeData();\r\n  },\r\n  methods: {\r\n    async fetchHomeData() {\r\n      try {\r\n        const response = await api.get(\"/api/home\");\r\n        const result = response.data;\r\n        if (result.status === 200 && !result.errorCode) {\r\n          this.homeData = result.data;\r\n          console.log(\"홈 데이터 조회 성공:\", this.homeData);\r\n        } else {\r\n          console.error(\"홈 데이터 조회 실패:\", result);\r\n        }\r\n      } catch (error) {\r\n        console.error(\"홈 데이터 조회 오류:\", error.response?.data || error.message);\r\n      }\r\n    },\r\n    logout() {\r\n      localStorage.removeItem(\"accessToken\");\r\n      localStorage.removeItem(\"refreshToken\");\r\n      this.$router.push(\"/\");\r\n      window.location.reload();\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.home-container {\r\n  max-width: 800px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n}\r\nheader nav {\r\n  text-align: right;\r\n  margin-bottom: 20px;\r\n}\r\nnav a {\r\n  margin: 0 10px;\r\n}\r\nh3 {\r\n  margin-top: 20px;\r\n}\r\nul {\r\n  list-style: none;\r\n  padding: 0;\r\n}\r\nli {\r\n  padding: 10px 0;\r\n  border-bottom: 1px solid #eee;\r\n}\r\n</style>"],"mappings":";AAsCA,OAAOA,GAAE,MAAO,QAAQ;AAExB,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,UAAUA,CAAA,EAAG;MACX,MAAMC,KAAI,GAAIC,YAAY,CAACC,OAAO,CAAC,aAAa,CAAC;MACjDC,OAAO,CAACC,GAAG,CAAC,gCAAgC,EAAEJ,KAAK,CAAC;IACtD;EACF,CAAC;EACDK,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,aAAa,CAAC,CAAC;EACtB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,aAAaA,CAAA,EAAG;MACpB,IAAI;QACF,MAAME,QAAO,GAAI,MAAMd,GAAG,CAACe,GAAG,CAAC,WAAW,CAAC;QAC3C,MAAMC,MAAK,GAAIF,QAAQ,CAACZ,IAAI;QAC5B,IAAIc,MAAM,CAACC,MAAK,KAAM,GAAE,IAAK,CAACD,MAAM,CAACE,SAAS,EAAE;UAC9C,IAAI,CAACf,QAAO,GAAIa,MAAM,CAACd,IAAI;UAC3BO,OAAO,CAACC,GAAG,CAAC,cAAc,EAAE,IAAI,CAACP,QAAQ,CAAC;QAC5C,OAAO;UACLM,OAAO,CAACU,KAAK,CAAC,cAAc,EAAEH,MAAM,CAAC;QACvC;MACF,EAAE,OAAOG,KAAK,EAAE;QACdV,OAAO,CAACU,KAAK,CAAC,cAAc,EAAEA,KAAK,CAACL,QAAQ,EAAEZ,IAAG,IAAKiB,KAAK,CAACC,OAAO,CAAC;MACtE;IACF,CAAC;IACDC,MAAMA,CAAA,EAAG;MACPd,YAAY,CAACe,UAAU,CAAC,aAAa,CAAC;MACtCf,YAAY,CAACe,UAAU,CAAC,cAAc,CAAC;MACvC,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,GAAG,CAAC;MACtBC,MAAM,CAACC,QAAQ,CAACC,MAAM,CAAC,CAAC;IAC1B;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}