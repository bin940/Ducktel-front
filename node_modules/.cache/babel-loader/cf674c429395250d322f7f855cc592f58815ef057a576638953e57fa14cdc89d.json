{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport api from \"../api\";\nexport default {\n  name: \"HomePage\",\n  data() {\n    return {\n      homeData: null,\n      selectedCategory: \"\"\n    };\n  },\n  computed: {\n    isLoggedIn() {\n      const token = localStorage.getItem(\"accessToken\");\n      console.log(\"홈페이지 - 로그인 상태 확인, accessToken:\", token);\n      return !!token;\n    }\n  },\n  created() {\n    this.fetchHomeData();\n  },\n  methods: {\n    async fetchHomeData() {\n      try {\n        const response = await api.get(\"/api/home\");\n        const result = response.data;\n        if (result.status === 200 && !result.errorCode) {\n          this.homeData = result.data;\n          console.log(\"홈 데이터 조회 성공:\", this.homeData);\n        } else {\n          console.error(\"홈 데이터 조회 실패:\", result);\n        }\n      } catch (error) {\n        console.error(\"홈 데이터 조회 오류:\", error.response?.data || error.message);\n      }\n    },\n    logout() {\n      localStorage.removeItem(\"accessToken\");\n      localStorage.removeItem(\"refreshToken\");\n      this.$router.push(\"/\");\n    },\n    goToSubHome() {\n      if (this.selectedCategory) {\n        this.$router.push(`/sub-home/${this.selectedCategory}`);\n      }\n    }\n  }\n};","map":{"version":3,"names":["api","name","data","homeData","selectedCategory","computed","isLoggedIn","token","localStorage","getItem","console","log","created","fetchHomeData","methods","response","get","result","status","errorCode","error","message","logout","removeItem","$router","push","goToSubHome"],"sources":["C:\\Users\\mycoms\\Desktop\\study\\duckfront\\src\\components\\HomePage.vue"],"sourcesContent":["<template>\r\n  <div class=\"home-container\">\r\n    <header>\r\n      <nav>\r\n        <router-link v-if=\"!isLoggedIn\" to=\"/login\">로그인</router-link>\r\n        <router-link v-if=\"!isLoggedIn\" to=\"/register\">회원가입</router-link>\r\n        <template v-if=\"isLoggedIn\">\r\n          <router-link to=\"/mypage\">마이페이지</router-link> |\r\n          <a href=\"#\" @click.prevent=\"logout\">로그아웃</a>\r\n        </template>\r\n      </nav>\r\n    </header>\r\n    <h2>홈페이지</h2>\r\n    <div>\r\n      <label for=\"category\">카테고리 선택:</label>\r\n      <select id=\"category\" v-model=\"selectedCategory\" @change=\"goToSubHome\">\r\n        <option value=\"\" disabled>카테고리를 선택하세요</option>\r\n        <option value=\"hotel\">호텔</option>\r\n        <option value=\"resort\">리조트</option>\r\n        <option value=\"pension\">펜션</option>\r\n        <option value=\"guesthouse\">게스트하우스</option>\r\n        <option value=\"motel\">모텔</option>\r\n      </select>\r\n    </div>\r\n    <div v-if=\"homeData\">\r\n      <h3 v-if=\"homeData.discountAccommodations?.length\">할인 숙소</h3>\r\n      <ul v-if=\"homeData.discountAccommodations?.length\">\r\n        <li v-for=\"item in homeData.discountAccommodations\" :key=\"item.accommodationId\">\r\n          {{ item.name }} - {{ item.location }} (할인: {{ item.discount }}%)\r\n        </li>\r\n      </ul>\r\n      <h3 v-if=\"homeData.favoriteAccommodations?.length\">인기 숙소</h3>\r\n      <ul v-if=\"homeData.favoriteAccommodations?.length\">\r\n        <li v-for=\"item in homeData.favoriteAccommodations\" :key=\"item.accommodationId\">\r\n          {{ item.name }} - {{ item.location }} (좋아요: {{ item.likeCount }})\r\n        </li>\r\n      </ul>\r\n      <h3 v-if=\"homeData.seasonalAccommodations?.length\">계절별 숙소</h3>\r\n      <ul v-if=\"homeData.seasonalAccommodations?.length\">\r\n        <li v-for=\"item in homeData.seasonalAccommodations\" :key=\"item.accommodationId\">\r\n          {{ item.name }} - {{ item.season }}\r\n        </li>\r\n      </ul>\r\n    </div>\r\n    <p v-else>데이터를 불러오는 중...</p>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport api from \"../api\";\r\n\r\nexport default {\r\n  name: \"HomePage\",\r\n  data() {\r\n    return {\r\n      homeData: null,\r\n      selectedCategory: \"\",\r\n    };\r\n  },\r\n  computed: {\r\n    isLoggedIn() {\r\n      const token = localStorage.getItem(\"accessToken\");\r\n      console.log(\"홈페이지 - 로그인 상태 확인, accessToken:\", token);\r\n      return !!token;\r\n    },\r\n  },\r\n  created() {\r\n    this.fetchHomeData();\r\n  },\r\n  methods: {\r\n    async fetchHomeData() {\r\n      try {\r\n        const response = await api.get(\"/api/home\");\r\n        const result = response.data;\r\n        if (result.status === 200 && !result.errorCode) {\r\n          this.homeData = result.data;\r\n          console.log(\"홈 데이터 조회 성공:\", this.homeData);\r\n        } else {\r\n          console.error(\"홈 데이터 조회 실패:\", result);\r\n        }\r\n      } catch (error) {\r\n        console.error(\"홈 데이터 조회 오류:\", error.response?.data || error.message);\r\n      }\r\n    },\r\n    logout() {\r\n      localStorage.removeItem(\"accessToken\");\r\n      localStorage.removeItem(\"refreshToken\");\r\n      this.$router.push(\"/\");\r\n    },\r\n    goToSubHome() {\r\n      if (this.selectedCategory) {\r\n        this.$router.push(`/sub-home/${this.selectedCategory}`);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.home-container {\r\n  max-width: 800px;\r\n  margin: 50px auto;\r\n  padding: 20px;\r\n}\r\nselect {\r\n  padding: 5px;\r\n  margin-left: 10px;\r\n}\r\n</style>"],"mappings":";AAiDA,OAAOA,GAAE,MAAO,QAAQ;AAExB,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,IAAI;MACdC,gBAAgB,EAAE;IACpB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,UAAUA,CAAA,EAAG;MACX,MAAMC,KAAI,GAAIC,YAAY,CAACC,OAAO,CAAC,aAAa,CAAC;MACjDC,OAAO,CAACC,GAAG,CAAC,gCAAgC,EAAEJ,KAAK,CAAC;MACpD,OAAO,CAAC,CAACA,KAAK;IAChB;EACF,CAAC;EACDK,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,aAAa,CAAC,CAAC;EACtB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,aAAaA,CAAA,EAAG;MACpB,IAAI;QACF,MAAME,QAAO,GAAI,MAAMf,GAAG,CAACgB,GAAG,CAAC,WAAW,CAAC;QAC3C,MAAMC,MAAK,GAAIF,QAAQ,CAACb,IAAI;QAC5B,IAAIe,MAAM,CAACC,MAAK,KAAM,GAAE,IAAK,CAACD,MAAM,CAACE,SAAS,EAAE;UAC9C,IAAI,CAAChB,QAAO,GAAIc,MAAM,CAACf,IAAI;UA