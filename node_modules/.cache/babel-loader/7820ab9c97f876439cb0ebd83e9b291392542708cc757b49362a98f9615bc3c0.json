{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport api from \"../api\";\nexport default {\n  name: \"HomePage\",\n  data() {\n    return {\n      homeData: null,\n      selectedCategory: \"\",\n      checkInDate: new Date().toISOString().split(\"T\")[0],\n      checkOutDate: new Date(Date.now() + 86400000).toISOString().split(\"T\")[0]\n    };\n  },\n  computed: {\n    isLoggedIn() {\n      return !!localStorage.getItem(\"accessToken\");\n    }\n  },\n  created() {\n    this.fetchHomeData();\n  },\n  methods: {\n    async fetchHomeData() {\n      try {\n        const response = await api.get(\"/api/home\");\n        const result = response.data;\n        if (result.status === 200 && !result.errorCode) {\n          this.homeData = result.data;\n          console.log(\"홈 데이터 조회 성공:\", this.homeData);\n        } else {\n          console.error(\"홈 데이터 조회 실패:\", result);\n        }\n      } catch (error) {\n        console.error(\"홈 데이터 조회 오류:\", error.response?.data || error.message);\n      }\n    },\n    logout() {\n      localStorage.removeItem(\"accessToken\");\n      localStorage.removeItem(\"refreshToken\");\n      this.$router.push(\"/\");\n    },\n    goToSubHome() {\n      if (this.selectedCategory) {\n        this.$router.push(`/sub-home/${this.selectedCategory}`);\n      }\n    },\n    goToPlaces(accommodationId) {\n      console.log(\"Navigating to:\", `/places/${accommodationId}/${this.checkInDate}/${this.checkOutDate}`);\n      this.$router.push(`/places/${accommodationId}/${this.checkInDate}/${this.checkOutDate}`);\n    }\n  }\n};","map":{"version":3,"names":["api","name","data","homeData","selectedCategory","checkInDate","Date","toISOString","split","checkOutDate","now","computed","isLoggedIn","localStorage","getItem","created","fetchHomeData","methods","response","get","result","status","errorCode","console","log","error","message","logout","removeItem","$router","push","goToSubHome","goToPlaces","accommodationId"],"sources":["C:\\Users\\mycoms\\Desktop\\study\\duckfront\\src\\components\\HomePage.vue"],"sourcesContent":["<template>\r\n  <div class=\"home-container\">\r\n    \r\n    <h2>홈페이지</h2>\r\n    <div>\r\n      <label for=\"category\">카테고리 선택:</label>\r\n      <select id=\"category\" v-model=\"selectedCategory\" @change=\"goToSubHome\">\r\n        <option value=\"\" disabled>카테고리를 선택하세요</option>\r\n        <option value=\"호텔\">호텔</option>\r\n        <option value=\"리조트\">리조트</option>\r\n        <option value=\"펜션\">펜션</option>\r\n        <option value=\"게스트하우스\">게스트하우스</option>\r\n        <option value=\"모텔\">모텔</option>\r\n      </select>\r\n    </div>\r\n\r\n    <!-- 할인 숙소 -->\r\n    <div v-if=\"homeData && homeData.discountAccommodations\">\r\n      <h3>할인 숙소</h3>\r\n      <div class=\"accommodations-container\">\r\n        <div\r\n          v-for=\"item in homeData.discountAccommodations\"\r\n          :key=\"item.accommodationId\"\r\n          class=\"accommodation-card\"\r\n          @click=\"goToPlaces(item.accommodationId)\"\r\n        >\r\n          <img\r\n            v-if=\"item.image && item.image.length\"\r\n            :src=\"item.image[0]\"\r\n            alt=\"숙소 이미지\"\r\n            class=\"accommodation-image\"\r\n          />\r\n          <div class=\"accommodation-details\">\r\n            <strong>{{ item.name }}</strong>\r\n            <p>{{ item.location }}</p>\r\n            <p v-if=\"item.discount\">할인: {{ item.discount }}%</p>\r\n            <p>좋아요: {{ item.likeCount }}</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 인기 숙소 -->\r\n    <div v-if=\"homeData && homeData.favoriteAccommodations\">\r\n      <h3>인기 숙소</h3>\r\n      <div class=\"accommodations-container\">\r\n        <div\r\n          v-for=\"item in homeData.favoriteAccommodations\"\r\n          :key=\"item.accommodationId\"\r\n          class=\"accommodation-card\"\r\n          @click=\"goToPlaces(item.accommodationId)\"\r\n        >\r\n          <img\r\n            v-if=\"item.image && item.image.length\"\r\n            :src=\"item.image[0]\"\r\n            alt=\"숙소 이미지\"\r\n            class=\"accommodation-image\"\r\n          />\r\n          <div class=\"accommodation-details\">\r\n            <strong>{{ item.name }}</strong>\r\n            <p>{{ item.location }}</p>\r\n            <p v-if=\"item.discount\">할인: {{ item.discount }}%</p>\r\n            <p>좋아요: {{ item.likeCount }}</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 계절 추천 숙소 -->\r\n    <div v-if=\"homeData && homeData.seasonalAccommodations\">\r\n      <h3>계절 추천 숙소</h3>\r\n      <div class=\"accommodations-container\">\r\n        <div\r\n          v-for=\"item in homeData.seasonalAccommodations\"\r\n          :key=\"item.accommodationId\"\r\n          class=\"accommodation-card\"\r\n          @click=\"goToPlaces(item.accommodationId)\"\r\n        >\r\n          <img\r\n            v-if=\"item.image && item.image.length\"\r\n            :src=\"item.image[0]\"\r\n            alt=\"숙소 이미지\"\r\n            class=\"accommodation-image\"\r\n          />\r\n          <div class=\"accommodation-details\">\r\n            <strong>{{ item.name }}</strong>\r\n            <p>{{ item.location }}</p>\r\n            <p v-if=\"item.discount\">할인: {{ item.discount }}%</p>\r\n            <p>좋아요: {{ item.likeCount }}</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <p v-if=\"!homeData\">데이터를 불러오는 중...</p>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport api from \"../api\";\r\n\r\nexport default {\r\n  name: \"HomePage\",\r\n  data() {\r\n    return {\r\n      homeData: null,\r\n      selectedCategory: \"\",\r\n      checkInDate: new Date().toISOString().split(\"T\")[0],\r\n      checkOutDate: new Date(Date.now() + 86400000).toISOString().split(\"T\")[0],\r\n    };\r\n  },\r\n  computed: {\r\n    isLoggedIn() {\r\n      return !!localStorage.getItem(\"accessToken\");\r\n    },\r\n  },\r\n  created() {\r\n    this.fetchHomeData();\r\n  },\r\n  methods: {\r\n    async fetchHomeData() {\r\n      try {\r\n        const response = await api.get(\"/api/home\");\r\n        const result = response.data;\r\n        if (result.status === 200 && !result.errorCode) {\r\n          this.homeData = result.data;\r\n          console.log(\"홈 데이터 조회 성공:\", this.homeData);\r\n        } else {\r\n          console.error(\"홈 데이터 조회 실패:\", result);\r\n        }\r\n      } catch (error) {\r\n        console.error(\"홈 데이터 조회 오류:\", error.response?.data || error.message);\r\n      }\r\n    },\r\n    logout() {\r\n      localStorage.removeItem(\"accessToken\");\r\n      localStorage.removeItem(\"refreshToken\");\r\n      this.$router.push(\"/\");\r\n    },\r\n    goToSubHome() {\r\n      if (this.selectedCategory) {\r\n        this.$router.push(`/sub-home/${this.selectedCategory}`);\r\n      }\r\n    },\r\n    goToPlaces(accommodationId) {\r\n      console.log(\"Navigating to:\", `/places/${accommodationId}/${this.checkInDate}/${this.checkOutDate}`);\r\n      this.$router.push(`/places/${accommodationId}/${this.checkInDate}/${this.checkOutDate}`);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.home-container {\r\n  max-width: 1200px;\r\n  margin: 50px auto;\r\n  padding: 20px;\r\n}\r\n.accommodations-container {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  gap: 20px;\r\n}\r\n.accommodation-card {\r\n  width: 220px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 5px;\r\n  padding: 10px;\r\n  cursor: pointer;\r\n  transition: transform 0.2s;\r\n}\r\n.accommodation-card:hover {\r\n  transform: scale(1.05);\r\n}\r\n.accommodation-image {\r\n  width: 100%;\r\n  height: 150px;\r\n  object-fit: cover;\r\n  border-radius: 5px;\r\n}\r\n.accommodation-details {\r\n  text-align: center;\r\n}\r\nselect {\r\n  padding: 5px;\r\n  margin-left: 10px;\r\n}\r\n</style>"],"mappings":";AAmGA,OAAOA,GAAE,MAAO,QAAQ;AAExB,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,