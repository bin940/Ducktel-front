aces","accommodationId","prevPage","type","nextPage"],"sources":["C:\\Users\\mycoms\\Desktop\\study\\duckfront\\src\\components\\HomePage.vue"],"sourcesContent":["<template>\r\n  <div class=\"home-container\">\r\n    <header>\r\n      <nav>\r\n        <router-link v-if=\"!isLoggedIn\" to=\"/login\">로그인</router-link>\r\n        <router-link v-if=\"!isLoggedIn\" to=\"/register\">회원가입</router-link>\r\n        <template v-if=\"isLoggedIn\">\r\n          <router-link to=\"/mypage\">마이페이지</router-link> |\r\n          <a href=\"#\" @click.prevent=\"logout\">로그아웃</a>\r\n        </template>\r\n      </nav>\r\n    </header>\r\n    <h2>홈페이지</h2>\r\n    <div>\r\n      <label for=\"category\">카테고리 선택:</label>\r\n      <select id=\"category\" v-model=\"selectedCategory\" @change=\"goToSubHome\">\r\n        <option value=\"\" disabled>카테고리를 선택하세요</option>\r\n        <option value=\"호텔\">호텔</option>\r\n        <option value=\"리조트\">리조트</option>\r\n        <option value=\"펜션\">펜션</option>\r\n        <option value=\"게스트하우스\">게스트하우스</option>\r\n        <option value=\"모텔\">모텔</option>\r\n      </select>\r\n    </div>\r\n\r\n    <!-- 할인 숙소 -->\r\n    <div v-if=\"homeData && homeData.discountAccommodations\">\r\n      <h3>할인 숙소</h3>\r\n      <div class=\"accommodations-container\">\r\n        <div\r\n          v-for=\"item in paginatedDiscount\"\r\n          :key=\"item.accommodationId\"\r\n          class=\"accommodation-card\"\r\n          @click=\"goToPlaces(item.accommodationId)\"\r\n        >\r\n          <img\r\n            v-if=\"item.image && item.image.length\"\r\n            :src=\"item.image[0]\"\r\n            alt=\"숙소 이미지\"\r\n            class=\"accommodation-image\"\r\n          />\r\n          <div class=\"accommodation-details\">\r\n            <strong>{{ item.name }}</strong>\r\n            <p>{{ item.location }}</p>\r\n            <p v-if=\"item.discount\">할인: {{ item.discount }}%</p>\r\n            <p>좋아요: {{ item.likeCount }}</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"pagination\">\r\n        <button @click=\"prevPage('discount')\" :disabled=\"discountPage === 1\">이전</button>\r\n        <span>페이지 {{ discountPage }} / {{ discountPages }}</span>\r\n        <button @click=\"nextPage('discount')\" :disabled=\"discountPage === discountPages\">다음</button>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 인기 숙소 -->\r\n    <div v-if=\"homeData && homeData.favoriteAccommodations\">\r\n      <h3>인기 숙소</h3>\r\n      <div class=\"accommodations-container\">\r\n        <div\r\n          v-for=\"item in paginatedFavorite\"\r\n          :key=\"item.accommodationId\"\r\n          class=\"accommodation-card\"\r\n          @click=\"goToPlaces(item.accommodationId)\"\r\n        >\r\n          <img\r\n            v-if=\"item.image && item.image.length\"\r\n            :src=\"item.image[0]\"\r\n            alt=\"숙소 이미지\"\r\n            class=\"accommodation-image\"\r\n          />\r\n          <div class=\"accommodation-details\">\r\n            <strong>{{ item.name }}</strong>\r\n            <p>{{ item.location }}</p>\r\n            <p v-if=\"item.discount\">할인: {{ item.discount }}%</p>\r\n            <p>좋아요: {{ item.likeCount }}</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"pagination\">\r\n        <button @click=\"prevPage('favorite')\" :disabled=\"favoritePage === 1\">이전</button>\r\n        <span>페이지 {{ favoritePage }} / {{ favoritePages }}</span>\r\n        <button @click=\"nextPage('favorite')\" :disabled=\"favoritePage === favoritePages\">다음</button>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 계절 추천 숙소 -->\r\n    <div v-if=\"homeData && homeData.seasonalAccommodations\">\r\n      <h3>계절 추천 숙소</h3>\r\n      <div class=\"accommodations-container\">\r\n        <div\r\n          v-for=\"item in paginatedSeasonal\"\r\n          :key=\"item.accommodationId\"\r\n          class=\"accommodation-card\"\r\n          @click=\"goToPlaces(item.accommodationId)\"\r\n        >\r\n          <img\r\n            v-if=\"item.image && item.image.length\"\r\n            :src=\"item.image[0]\"\r\n            alt=\"숙소 이미지\"\r\n            class=\"accommodation-image\"\r\n          />\r\n          <div class=\"accommodation-details\">\r\n            <strong>{{ item.name }}</strong>\r\n            <p>{{ item.location }}</p>\r\n            <p v-if=\"item.discount\">할인: {{ item.discount }}%</p>\r\n            <p>좋아요: {{ item.likeCount }}</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"pagination\">\r\n        <button @click=\"prevPage('seasonal')\" :disabled=\"seasonalPage === 1\">이전</button>\r\n        <span>페이지 {{ seasonalPage }} / {{ seasonalPages }}</span>\r\n        <button @click=\"nextPage('seasonal')\" :disabled=\"seasonalPage === seasonalPages\">다음</button>\r\n      </div>\r\n    </div>\r\n\r\n    <p v-if=\"!homeData\">데이터를 불러오는 중...</p>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport api from \"../api\";\r\n\r\nexport default {\r\n  name: \"HomePage\",\r\n  data() {\r\n    return {\r\n      homeData: null,\r\n      selectedCategory: \"\",\r\n      checkInDate: new Date().toISOString().split(\"T\")[0],\r\n      checkOutDate: new Date(Date.now() + 86400000).toISOString().split(\"T\")[0],\r\n      discountPage: 1,\r\n      favoritePage: 1,\r\n      seasonalPage: 1,\r\n      itemsPerPage: 5,\r\n    };\r\n  },\r\n  computed: {\r\n    isLoggedIn() {\r\n      return !!localStorage.getItem(\"accessToken\");\r\n    },\r\n    paginatedDiscount() {\r\n      const start = (this.discountPage - 1) * this.itemsPerPage;\r\n      const end = start + this.itemsPerPage;\r\n      return this.homeData?.discountAccommodations?.slice(start, end) || [];\r\n    },\r\n    discountPages() {\r\n      return Math.ceil((this.homeData?.discountAccommodations?.length || 0) / this.itemsPerPage);\r\n    },\r\n    paginatedFavorite() {\r\n      const start = (this.favoritePage - 1) * this.itemsPerPage;\r\n      const end = start + this.itemsPerPage;\r\n      return this.homeData?.favoriteAccommodations?.slice(start, end) || [];\r\n    },\r\n    favoritePages() {\r\n      return Math.ceil((this.homeData?.favoriteAccommodations?.length || 0) / this.itemsPerPage);\r\n    },\r\n    paginatedSeasonal() {\r\n      const start = (this.seasonalPage - 1) * this.itemsPerPage;\r\n      const end = start + this.itemsPerPage;\r\n      return this.homeData?.seasonalAccommodations?.slice(start, end) || [];\r\n    },\r\n    seasonalPages() {\r\n      return Math.ceil((this.homeData?.seasonalAccommodations?.length || 0) / this.itemsPerPage);\r\n    },\r\n  },\r\n  created() {\r\n    this.fetchHomeData();\r\n  },\r\n  methods: {\r\n    async fetchHomeData() {\r\n      try {\r\n        const response = await api.get(\"/api/home\");\r\n        const result = response.data;\r\n        if (result.status === 200 && !result.errorCode) {\r\n          this.homeData = result.data;\r\n          console.log(\"홈 데이터 조회 성공:\", this.homeData);\r\n        } else {\r\n          console.error(\"홈 데이터 조회 실패:\", result);\r\n        }\r\n      } catch (error) {\r\n        console.error(\"홈 데이터 조회 오류:\", error.response?.data || error.message);\r\n      }\r\n    },\r\n    logout() {\r\n      localStorage.removeItem(\"accessToken\");\r\n      localStorage.removeItem(\"refreshToken\");\r\n      this.$router.push(\"/\");\r\n    },\r\n    goToSubHome() {\r\n      if (this.selectedCategory) {\r\n        this.$router.push(`/sub-home/${this.selectedCategory}`);\r\n      }\r\n    },\r\n    goToPlaces(accommodationId) {\r\n      console.log(\"Navigating to:\", `/places/${accommodationId}/${this.checkInDate}/${this.checkOutDate}`);\r\n      this.$router.push(`/places/${accommodationId}/${this.checkInDate}/${this.checkOutDate}`);\r\n    },\r\n    prevPage(type) {\r\n      if (type === \"discount\" && this.discountPage > 1) this.discountPage--;\r\n      if (type === \"favorite\" && this.favoritePage > 1) this.favoritePage--;\r\n      if (type === \"seasonal\" 