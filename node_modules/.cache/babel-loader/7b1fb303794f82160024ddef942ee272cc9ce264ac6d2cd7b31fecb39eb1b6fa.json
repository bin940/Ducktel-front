{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport api from \"../api\";\nexport default {\n  name: \"HomePage\",\n  data() {\n    return {\n      homeData: null,\n      selectedCategory: \"\",\n      checkInDate: new Date().toISOString().split(\"T\")[0],\n      checkOutDate: new Date(Date.now() + 86400000).toISOString().split(\"T\")[0],\n      discountPage: 1,\n      favoritePage: 1,\n      seasonalPage: 1,\n      itemsPerPage: 5\n    };\n  },\n  computed: {\n    isLoggedIn() {\n      return !!localStorage.getItem(\"accessToken\");\n    },\n    paginatedDiscount() {\n      const start = (this.discountPage - 1) * this.itemsPerPage;\n      const end = start + this.itemsPerPage;\n      return this.homeData?.discountAccommodations?.slice(start, end) || [];\n    },\n    discountPages() {\n      return Math.ceil((this.homeData?.discountAccommodations?.length || 0) / this.itemsPerPage);\n    },\n    paginatedFavorite() {\n      const start = (this.favoritePage - 1) * this.itemsPerPage;\n      const end = start + this.itemsPerPage;\n      return this.homeData?.favoriteAccommodations?.slice(start, end) || [];\n    },\n    favoritePages() {\n      return Math.ceil((this.homeData?.favoriteAccommodations?.length || 0) / this.itemsPerPage);\n    },\n    paginatedSeasonal() {\n      const start = (this.seasonalPage - 1) * this.itemsPerPage;\n      const end = start + this.itemsPerPage;\n      return this.homeData?.seasonalAccommodations?.slice(start, end) || [];\n    },\n    seasonalPages() {\n      return Math.ceil((this.homeData?.seasonalAccommodations?.length || 0) / this.itemsPerPage);\n    }\n  },\n  created() {\n    this.fetchHomeData();\n  },\n  methods: {\n    async fetchHomeData() {\n      try {\n        const response = await api.get(\"/api/home\");\n        const result = response.data;\n        if (result.status === 200 && !result.errorCode) {\n          this.homeData = result.data;\n          console.log(\"홈 데이터 조회 성공:\", this.homeData);\n      