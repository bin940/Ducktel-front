{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport api from \"../api\";\nexport default {\n  name: \"LoginForm\",\n  data() {\n    return {\n      username: \"\",\n      password: \"\",\n      errorMessage: \"\"\n    };\n  },\n  methods: {\n    async handleLogin() {\n      this.errorMessage = \"\";\n      try {\n        const response = await api.post(\"/api/auth/login\", {\n          username: this.username,\n          password: this.password\n        });\n        const result = response.data;\n        // 응답 내부 status가 400 이상이면 에러로 처리\n        if (result.status >= 400) {\n          this.errorMessage = result.message;\n          console.error(\"로그인 실패:\", result);\n        } else {\n          console.log(\"로그인 성공:\", result);\n          // 성공 시 로직 (예: 페이지 이동)\n          this.$router.push(\"/home\");\n        }\n      } catch (error) {\n        const errorResponse = error.response?.data;\n        this.errorMessage = errorResponse?.message || \"로그인 중 오류가 발생했습니다.\";\n        console.error(\"로그인 실패:\", errorResponse || error.message);\n      }\n    }\n  }\n};","map":{"version":3,"names":["api","name","data","username","password","errorMessage","methods","handleLogin","response","post","result","status","message","console","error","log","$router","push","errorResponse"],"sources":["C:\\Users\\mycoms\\Desktop\\study\\duckfront\\src\\components\\LoginForm.vue"],"sourcesContent":["<template>\r\n  <div class=\"login-container\">\r\n    <h2>로그인</h2>\r\n    <form @submit.prevent=\"handleLogin\">\r\n      <div>\r\n        <label for=\"username\">아이디:</label>\r\n        <input\r\n          type=\"text\"\r\n          id=\"username\"\r\n          v-model=\"username\"\r\n          placeholder=\"아이디를 입력하세요\"\r\n        />\r\n      </div>\r\n      <div>\r\n        <label for=\"password\">비밀번호:</label>\r\n        <input\r\n          type=\"password\"\r\n          id=\"password\"\r\n          v-model=\"password\"\r\n          placeholder=\"비밀번호를 입력하세요\"\r\n        />\r\n      </div>\r\n      <button type=\"submit\">로그인</button>\r\n    </form>\r\n    <!-- 에러 메시지 표시 -->\r\n    <p v-if=\"errorMessage\" class=\"error-message\">{{ errorMessage }}</p>\r\n    <p>\r\n      계정이 없으신가요?\r\n      <a href=\"/register\">회원가입</a>\r\n    </p>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport api from \"../api\";\r\n\r\nexport default {\r\n  name: \"LoginForm\",\r\n  data() {\r\n    return {\r\n      username: \"\",\r\n      password: \"\",\r\n      errorMessage: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    async handleLogin() {\r\n      this.errorMessage = \"\";\r\n      try {\r\n        const response = await api.post(\"/api/auth/login\", {\r\n          username: this.username,\r\n          password: this.password,\r\n        });\r\n        const result = response.data;\r\n        // 응답 내부 status가 400 이상이면 에러로 처리\r\n        if (result.status >= 400) {\r\n          this.errorMessage = result.message;\r\n          console.error(\"로그인 실패:\", result);\r\n        } else {\r\n          console.log(\"로그인 성공:\", result);\r\n          // 성공 시 로직 (예: 페이지 이동)\r\n          this.$router.push(\"/home\");\r\n        }\r\n      } catch (error) {\r\n        const errorResponse = error.response?.data;\r\n        this.errorMessage = errorResponse?.message || \"로그인 중 오류가 발생했습니다.\";\r\n        console.error(\"로그인 실패:\", errorResponse || error.message);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.login-container {\r\n  max-width: 400px;\r\n  margin: 50px auto;\r\n  padding: 20px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 5px;\r\n}\r\ndiv {\r\n  margin-bottom: 15px;\r\n}\r\nlabel {\r\n  display: block;\r\n  margin-bottom: 5px;\r\n}\r\ninput {\r\n  width: 100%;\r\n  padding: 8px;\r\n  box-sizing: border-box;\r\n}\r\nbutton {\r\n  width: 100%;\r\n  padding: 10px;\r\n  background-color: #007bff;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\nbutton:hover {\r\n  background-color: #0056b3;\r\n}\r\n/* 에러 메시지 스타일 */\r\n.error-message {\r\n  color: red;\r\n  margin-top: 10px;\r\n  font-size: 14px;\r\n}\r\n</style>"],"mappings":";AAkCA,OAAOA,GAAE,MAAO,QAAQ;AAExB,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,WAAWA,CAAA,EAAG;MAClB,IAAI,CAACF,YAAW,GAAI,EAAE;MACtB,IAAI;QACF,MAAMG,QAAO,GAAI,MAAMR,GAAG,CAACS,IAAI,CAAC,iBAAiB,EAAE;UACjDN,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,CAAC;QACF,MAAMM,MAAK,GAAIF,QAAQ,CAACN,IAAI;QAC5B;QACA,IAAIQ,MAAM,CAACC,MAAK,IAAK,GAAG,EAAE;UACxB,IAAI,CAACN,YAAW,GAAIK,MAAM,CAACE,OAAO;UAClCC,OAAO,CAACC,KAAK,CAAC,SAAS,EAAEJ,MAAM,CAAC;QAClC,OAAO;UACLG,OAAO,CAACE,GAAG,CAAC,SAAS,EAAEL,MAAM,CAAC;UAC9B;UACA,IAAI,CAACM,OAAO,CAACC,IAAI,CAAC,OAAO,CAAC;QAC5B;MACF,EAAE,OAAOH,KAAK,EAAE;QACd,MAAMI,aAAY,GAAIJ,KAAK,CAACN,QAAQ,EAAEN,IAAI;QAC1C,IAAI,CAACG,YAAW,GAAIa,aAAa,EAAEN,OAAM,IAAK,mBAAmB;QACjEC,OAAO,CAACC,KAAK,CAAC,SAAS,EAAEI,aAAY,IAAKJ,KAAK,CAACF,OAAO,CAAC;MAC1D;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}