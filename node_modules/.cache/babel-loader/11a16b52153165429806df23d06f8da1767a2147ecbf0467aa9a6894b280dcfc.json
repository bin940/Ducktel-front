{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport api from \"../api\";\nexport default {\n  name: \"SubHome\",\n  data() {\n    return {\n      subHomeData: null,\n      selectedLocation: \"\",\n      checkInDate: new Date().toISOString().split(\"T\")[0],\n      // 오늘\n      checkOutDate: new Date(Date.now() + 86400000).toISOString().split(\"T\")[0] // 내일\n    };\n  },\n  computed: {\n    category() {\n      return this.$route.params.category;\n    }\n  },\n  created() {\n    this.fetchSubHomeData();\n  },\n  methods: {\n    async fetchSubHomeData() {\n      try {\n        const response = await api.get(`/api/home/${this.category}`);\n        const result = response.data;\n        if (result.status === 200 && !result.errorCode) {\n          this.subHomeData = result.data;\n          console.log(\"Home 데이터 조회 성공:\", this.subHomeData);\n        } else {\n          console.error(\"Home 데이터 조회 실패:\", result);\n        }\n      } catch (error) {\n        console.error(\"Home 데이터 조회 오류:\", error.response?.data || error.message);\n      }\n    },\n    goToLocationHome() {\n      if (this.selectedLocation) {\n        this.$router.push(`/home/${this.category}/${this.selectedLocation}`);\n      }\n    },\n    goToPlaces(accommodationId) {\n      this.$router.push(`/places/${accommodationId}/${this.checkInDate}/${this.checkOutDate}`);\n    }\n  }\n};","map":{"version":3,"names":["api","name","data","subHomeData","selectedLocation","checkInDate","Date","toISOString","split","checkOutDate","now","computed","category","$route","params","created","fetchSubHomeData","methods","response","get","result","status","errorCode","console","log","error","message","goToLocationHome","$router","push","goToPlaces","accommodationId"],"sources":["C:\\Users\\mycoms\\Desktop\\study\\Ducktel\\duckfront\\src\\components\\CategoryHome.vue"],"sourcesContent":["<template>\r\n  <div class=\"sub-home-container\">\r\n    <h2>{{ category }} 페이지</h2>\r\n    <div>\r\n      <label for=\"location\">지역 선택:</label>\r\n      <select id=\"location\" v-model=\"selectedLocation\" @change=\"goToLocationHome\">\r\n        <option value=\"\" disabled>지역을 선택하세요</option>\r\n        <option value=\"서울 강남구\">서울 강남구</option>\r\n        <option value=\"부산 해운대\">부산 해운대</option>\r\n        <option value=\"제주 서귀포\">제주 서귀포</option>\r\n        <option value=\"강원도 평창\">강원도 평창</option>\r\n        <option value=\"서울 종로구\">서울 종로구</option>\r\n      </select>\r\n    </div>\r\n    <div v-if=\"subHomeData\">\r\n      <h3>{{ category }} 숙소 목록</h3>\r\n      <ul v-if=\"subHomeData.categoryAccommodations && subHomeData.categoryAccommodations.length\">\r\n        <li\r\n          v-for=\"item in subHomeData.categoryAccommodations\"\r\n          :key=\"item.accommodationId\"\r\n          @click=\"goToPlaces(item.accommodationId)\"\r\n          style=\"cursor: pointer;\"\r\n        >\r\n          <strong>{{ item.name }}</strong> - {{ item.location }}<br />\r\n          <img\r\n            v-if=\"item.image && item.image.length\"\r\n            :src=\"item.image[0]\"\r\n            alt=\"숙소 이미지\"\r\n            class=\"accommodation-image\"\r\n          /><br />\r\n          <span>태그: {{ item.tag || \"없음\" }}</span><br />\r\n          <span>설명: {{ item.explanation || \"없음\" }}</span><br />\r\n          <span>서비스: {{ item.serviceInfo || \"없음\" }}</span><br />\r\n          <span v-if=\"item.discount\">할인: {{ item.discount }}%</span><br />\r\n          <span v-if=\"item.season\">계절: {{ item.season }}</span><br />\r\n          <span>좋아요: {{ item.likeCount }}</span>\r\n        </li>\r\n      </ul>\r\n      <p v-else>{{ category }} 카테고리에 숙소가 없습니다.</p>\r\n    </div>\r\n    <p v-else>데이터를 불러오는 중...</p>\r\n    \r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport api from \"../api\";\r\n\r\nexport default {\r\n  name: \"SubHome\",\r\n  data() {\r\n    return {\r\n      subHomeData: null,\r\n      selectedLocation: \"\",\r\n      checkInDate: new Date().toISOString().split(\"T\")[0], // 오늘\r\n      checkOutDate: new Date(Date.now() + 86400000).toISOString().split(\"T\")[0], // 내일\r\n    };\r\n  },\r\n  computed: {\r\n    category() {\r\n      return this.$route.params.category;\r\n    },\r\n  },\r\n  created() {\r\n    this.fetchSubHomeData();\r\n  },\r\n  methods: {\r\n    async fetchSubHomeData() {\r\n      try {\r\n        const response = await api.get(`/api/home/${this.category}`);\r\n        const result = response.data;\r\n        if (result.status === 200 && !result.errorCode) {\r\n          this.subHomeData = result.data;\r\n          console.log(\"Home 데이터 조회 성공:\", this.subHomeData);\r\n        } else {\r\n          console.error(\"Home 데이터 조회 실패:\", result);\r\n        }\r\n      } catch (error) {\r\n        console.error(\"Home 데이터 조회 오류:\", error.response?.data || error.message);\r\n      }\r\n    },\r\n    goToLocationHome() {\r\n      if (this.selectedLocation) {\r\n        this.$router.push(`/home/${this.category}/${this.selectedLocation}`);\r\n      }\r\n    },\r\n    goToPlaces(accommodationId) {\r\n      this.$router.push(`/places/${accommodationId}/${this.checkInDate}/${this.checkOutDate}`);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.sub-home-container {\r\n  max-width: 800px;\r\n  margin: 50px auto;\r\n  padding: 20px;\r\n}\r\nselect {\r\n  padding: 5px;\r\n  margin-left: 10px;\r\n}\r\nul {\r\n  list-style: none;\r\n  padding: 0;\r\n}\r\nli {\r\n  margin-bottom: 20px;\r\n  padding: 10px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 5px;\r\n}\r\nli:hover {\r\n  background-color: #f5f5f5;\r\n}\r\n.accommodation-image {\r\n  width: 50%;\r\n  height: 150px;\r\n  object-fit: cover;\r\n  border-radius: 5px;\r\n}\r\n</style>"],"mappings":";AA8CA,OAAOA,GAAE,MAAO,QAAQ;AAExB,eAAe;EACbC,IAAI,EAAE,SAAS;EACfC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,IAAI;MACjBC,gBAAgB,EAAE,EAAE;MACpBC,WAAW,EAAE,IAAIC,IAAI,CAAC,CAAC,CAACC,WAAW,CAAC,CAAC,CAACC,KAAK,CAAC,GAAG,CAAC,CAAC,CAAC,CAAC;MAAE;MACrDC,YAAY,EAAE,IAAIH,IAAI,CAACA,IAAI,